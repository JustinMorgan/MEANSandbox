extends layout

append head
    //- TODO: Bundle
    script(src='//cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.15/angular.js')
    script(src='//cdnjs.cloudflare.com/ajax/libs/angular-ui/0.4.0/angular-ui.js')
    script(src='//cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.13/angular-ui-router.js')
    script(src='//cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.15/angular-resource.js')
    base(href='/apps/#{appName}')
append content
    include partials/navigation
    h1(ng-cloak) {{appName}}
    section(ng-cloak)
      ui-view
    != js('dynamic')
  